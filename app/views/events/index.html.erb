<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<div class="container">
  <div class="row">
    <% if @events.nil? %>
      <%= "There is no event" %>
    <% else %>
      <% @events.each do |event| %>
        <!-- <%= form_tag event_path(event.id), method: :post do %> -->
        <li><a href ="<%= event_path(event.id) %>"><%= event.name %></a></li>

        <!-- <% end %> -->
      <% end %>
    <% end %>
  </div>
</div>
